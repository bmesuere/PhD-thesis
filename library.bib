eprint = {99/1818-3551},
isbn = {0173-0835},
issn = {0173-0835},
journal = {Electrophoresis},
keywords = {Amino Acid Sequence,Amino Acids,Amino Acids: chemistry,Databases, Factual,Information Storage and Retrieval,Mass Spectrometry,Molecular Sequence Data,Molecular Weight,Nucleic Acids,Nucleic Acids: genetics,Probability,Protein Biosynthesis,Proteins,Proteins: chemistry},
number = {18},
pages = {3551--67},
pmid = {10612281},
primaryClass = {0173-0835},
title = {{Probability-based protein identification by searching sequence databases using mass spectrometry data.}},
