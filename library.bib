journal = {BMC bioinformatics},
keywords = {Comparative heat maps,Metagenomics RAST server,Models in MG-RAST,and PEGs - functions belonging to subsytem(s),mapped to subsystem,metabolic reconstructions - mapped to reactions in,public resource - automatic phylogenetic and funct},
pages = {386},
pmid = {18803844},
title = {{The metagenomics RAST server—a public resource for the automatic phylo- genetic and functional analysis of metagenomes}},
